{
  "manifest_version": 3,
  "name": "Simple Bookmark Manager",
  "version": "2.0.0",
  "description": "简洁的Chrome书签管理工具：查看、添加、编辑、删除书签",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "书签管理器"
  }
}